[General]
git = https://github.com/somaticio/neural-style
name = Somatic.io Demo for Neural Style deep learning project
paper = http://arxiv.org/some/neat/paper
author = somebody@someplace.org

[Initialize]
command = sh /home/ubuntu/neural-style/models/download_models.sh
workspace = /home/ubuntu/neural-style/somatic/init

[Train]
command = th /home/ubuntu/neural-style/neural_style.lua -gpu 0 -backend cudnn
workspace = /home/ubuntu/neural-style/somatic/train

[Test]
command = th /home/ubuntu/neural-style/neural_style.lua -gpu 0 -backend cudnn -style_image /home/ubuntu/neural-style/examples/inputs/escher_sphere.jpg -content_image /home/ubuntu/neural-style/examples/inputs/brad_pitt.jpg -output_image /home/ubuntu/neural-style/somatic/test
workspace = /home/ubuntu/neural-style/somatic/test

[AWSOptions]
shutdown_after_run = true
something_else = false
