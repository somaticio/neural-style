General:
  git: https://github.com/somaticio/neural-style.git
  name: Somatic.io Demo for Neural Style deep learning project
  paper: http://arxiv.org/some/neat/paper
  author: somebody@someplace.org


Train:
  command: /home/ubuntu/experiment/train.sh

Inference:
  command: /home/ubuntu/experiment/test.sh
  workspace: /home/ubuntu/experiment/somatic/
  input: file
  output: file
  mode: async
  load_model_parameter:
    flag:
      -data_dir

  parameters:
    -backend:
      type: string
      default: nn
      choices: nn,cudnn,clnn
    -optimizer:
      type: string
      default: lbfgs
      choices: adam,lbfgs
    -style_scale:
      default: 1.0
      type: float
    -content_weight:
      type: integer
      default: 5
    -style_weight:
      type: integer
      default: 100
    -style_image:
      type: file
      required: true
      flag: -style_image
    -content_image:
      type: file
      required: true
      flag: -content_image
    -num_iterations:
      type: integer
      default: 1500
      flag: -num_iterations
    -image_size:
      type: integer
      default: 512
